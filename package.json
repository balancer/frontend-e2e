{
  "name": "@balancer-labs/frontend-e2e",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "synpress": "SECRET_WORDS=\"test test test test test test test test test test test junk\" NETWORK_NAME=mainnet synpress run -cf synpress.json --config 'supportFile=synpress/support/index.ts'",
    "synpress:start": "npm run synpress -- -ne",
    "synpress:ci": "npm-run-all synpress synpress:report --continue-on-error",
    "synpress:report": "npm-run-all synpress:report:generate synpress:report:copy-assets",
    "synpress:report:generate": "marge synpress/results/mochawesome.json -o synpress/results",
    "synpress:report:copy-assets": "mkdir -p synpress/screenshots synpress/videos && cp -r synpress/screenshots synpress/results/assets && cp -r synpress/videos synpress/results/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balancer-labs/frontend-e2e.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/balancer-labs/frontend-e2e/issues"
  },
  "homepage": "https://github.com/balancer-labs/frontend-e2e#readme",
  "devDependencies": {
    "@synthetixio/synpress": "^1.2.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.17.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "cypress": "9.5.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{md,json,yaml,html}": "prettier --write"
  }
}
